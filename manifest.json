{
  "manifest_version": 3,
  "name": "Tab Switcher",
  "version": "1.0",
  "permissions": ["tabs", "storage", "history"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "commands": {
    "open-tab-switcher": {
      "suggested_key": {
        "default": "Ctrl+E"
      },
      "description": "Open Tab Switcher"
    }
  }
}


